    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/ui": "^0.27.0",
    "eslint": "^8.34.0",
    "eslint-config-react-app": "^7.0.1",
    "jsdom": "^21.0.0",
    "prettier": "^2.8.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.27.0"
